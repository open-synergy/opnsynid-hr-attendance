<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>

        <record id="hr_timesheet_mass_generate_view_form" model="ir.ui.view">
            <field
                name="name"
            >hr.timesheet.mass.generate.attendance.schedule.form</field>
            <field name="model">hr.timesheet_mass_generate</field>
            <field
                name="inherit_id"
                ref="hr_timesheet_mass_generate.hr_timesheet_mass_generate_view_form"
            />
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//field[@name='date_to']" position="after">
                        <field name="working_schedule_id" />
                    </xpath>
                </data>
            </field>
        </record>

    </data>
</openerp>
